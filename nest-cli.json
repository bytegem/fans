      "sourceRoot": "libs/tencent-cloud-sts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/tencent-cloud-sts/tsconfig.lib.json"
      }
    },
    "client-server": {
      "type": "application",
      "root": "apps/client-server",
      "entryFile": "__dev_server",
      "sourceRoot": "apps/client-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client-server/tsconfig.app.json"
      }
    }
  }
}